<!DOCTYPE html>
<html>
<head>
	<title>Page Title</title>
</head>
<body>
   <h1>Halaman Profile</h1>

	<table border = "1">
         <tr>
         	<td>id</td>
            <td>Username</td>
            <td>Password</td>
            <td>Nama</td>
         </tr>
         
         
         	<%for(var i = 0; i < dataProfiles.length; i++){%>
        <tr>
            <td><%=dataProfiles[i].id%></td>
            <td><%=dataProfiles[i].username%></td>
            <td><%=dataProfiles[i].password%></td>
            <td><%=dataProfiles[i].name%></td>

         
            <td><a href="/profiles/edit/<%= dataProfiles[i].id %>">Edit</a></td>
            <td><a href="/profiles/delete/<%= dataProfiles[i].id %>">Delete</a></td>
        </tr>
            <%}%>
        
      </table>

	<p>Masukan data baru <p>
	<form action="/profiles" method="post">
      
         <select name="contact_id">
            <%for(var i = 0; i < dataContacts.length; i++){%>
               <option value="<%=dataContacts[i].id%>"><%=dataContacts[i].id%> <%=dataContacts[i].name%></option>
            <%}%>
         </select>
       
       <br>
         Username: <input type = "text" name = "username" />
         <br>
         Password: <input type = "text" name = "password" />
         <br>
         <input type = "submit" name = "submit" value = "Submit" />
      </form>


	


</body>
</html>
